<h1> Cat Index Page </h1>
<ul> 
    <%@cats.each do |cat|%>
    <a href= "<%= cat_url(cat) %>"> Cat Show Page </a>

    <li> Name: <%= cat.name %> </li>
    <li> Birthdate: <%= cat.birthdate %> </li>
    <li> Color: <%= cat.color %> </li>
    <li> Sex: <%= cat.sex %> </li>
    <li> Description: <%= cat.description %> </li>

    <% end %>
</ul>